  "type": "module",
  "homepage": "",
  "author": "Teihden",
  "private": true,
  "license": "ISC",
  "sideEffects": false,
  "scripts": {
    "start": "",
    "build": "",
    "lint": "npm run stylelint && npm run eslint",
    "stylelint": "stylelint --config stylelint.config.mjs ./src/**/*.{css,pcss,scss,sass,less} --fix",
    "eslint": "eslint --no-config-lookup --config eslint.config.ts ./src --fix",
    "eslint-config-inspect": "npx @eslint/config-inspector --config eslint.config.ts",
    "check-deps-updates": "npx npm-check-updates",
    "update-deps": "npx npm-check-updates -u && npm install"
  },
  "imports": {
    "@public/*": "./public/*",
    "@app/*": "./src/app/*",
    "@entities/*": "./src/entities/*",
    "@features/*": "./src/features/*",
    "@pages/*": "./src/pages/*",
    "@shared/*": "./src/shared/*",
    "@widgets/*": "./src/widgets/*"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@eslint/config-inspector": "^1.3.0",
    "@eslint/js": "^9.37.0",
    "@eslint/json": "^0.13.2",
    "@eslint/markdown": "^7.4.0",
    "@stylistic/eslint-plugin": "^5.4.0",
    "@stylistic/stylelint-config": "^3.0.1",
    "@stylistic/stylelint-plugin": "^4.0.0",
    "@types/eslint": "^9.6.1",
    "@types/espree": "^10.1.0",
    "@types/node": "^24.7.2",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.1",
